{
  "name": "Accent Mark Helper",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "A simple extension that helps a user type letters with accent marks.",
  "content_scripts": [{
    "matches": [
      "https://*/*"
    ],
    "exclude_globs": [
      "https://*.babbel.com/*"
    ],
    "js": [
      "src/keypressListener.js"
    ]
  }],
  "icons": {
    "128": "src/128x128-icon.png"
  }
}
